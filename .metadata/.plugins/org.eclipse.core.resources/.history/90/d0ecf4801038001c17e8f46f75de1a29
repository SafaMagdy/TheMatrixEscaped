
public class Grid {
	
	//create an object to contain the x and y of busy cells
	
	public static String genGrid() {
		String grid = "";
		//should be randomized
		int m = 3;
		int n = 3;
		int c = 1;
		int neoX = 0;
		int neoY = 0;
		grid = m + "," + n +";" + c + ";Neo" + neoX + ",Neo" + neoY + ";";
		int teleX = 1;
		int teleY = 1;
		grid += "Telephone" + teleX + ",Telephone" + teleY;
		int agentNumber = 1;
		for(int i = 0; i < agentNumber; i++) {
			int agentX = 0;
			int agentY = 1;
			grid += ";Agent" + agentX + ",Agent" + agentY;
		}
		return grid;
	}

	public static void main(String[] args) {
		System.out.println(grid);
	}
}
